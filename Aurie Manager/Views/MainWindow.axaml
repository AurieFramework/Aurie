<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Aurie_Manager.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Aurie_Manager.Views"
        mc:Ignorable="d" d:DesignWidth="480" d:DesignHeight="360"
        x:Class="Aurie_Manager.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="{Binding p_WindowTitle}"
		Width="{Binding p_WindowWidth}" Height="{Binding p_WindowHeight}"
		CanResize="False">
	<views:MainView />
</Window>
